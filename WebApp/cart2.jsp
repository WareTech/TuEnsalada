function SoloNumerico(e)
{
  var sKey = -1;
  var bResult = true;

  if (window.event)
  {
    sKey = e.keyCode;
  }
  else if (e.which)
  {
    sKey = e.which;
  }

  if (sKey != -1)
  {
    if (((sKey < 48) || (sKey > 57)) && sKey > 10)
    {
      bResult = false;
    }
  }
  return bResult;
}

